class Solution {
    public int arrangeCoins(int n) {
       long start =0;
       long end = n;
      long mid =0 , currSum;
while(start<= end){
    mid= start + (end-start)/2;
    currSum=mid*(mid+1)/2;
    if(currSum==n) return (int)mid;

    if(n<currSum){
       end=mid-1;
     }
  else{
    start=mid+1;
   }
}
return (int)end;
    }
}
